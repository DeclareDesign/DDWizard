    "tab_design-design_parameters": {
      "html": "<div>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_N\">N<\/label>\n    <input id=\"tab_design-design_arg_N\" type=\"number\" class=\"form-control\" value=\"100\" min=\"4\" step=\"1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_N', 'popover', {'content': 'Sample size', 'placement': 'top', 'trigger': 'hover', 'title': 'N'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_prob\">prob<\/label>\n    <input id=\"tab_design-design_arg_prob\" type=\"number\" class=\"form-control\" value=\"0.5\" min=\"0.1\" max=\"0.9\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_prob', 'popover', {'content': 'Probability of assignment to treatment', 'placement': 'top', 'trigger': 'hover', 'title': 'prob'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_control_mean\">control_mean<\/label>\n    <input id=\"tab_design-design_arg_control_mean\" type=\"number\" class=\"form-control\" value=\"0\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_control_mean', 'popover', {'content': 'Average outcome in control', 'placement': 'top', 'trigger': 'hover', 'title': 'control_mean'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_sd\">sd<\/label>\n    <input id=\"tab_design-design_arg_sd\" type=\"number\" class=\"form-control\" value=\"1\" min=\"0\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_sd', 'popover', {'content': 'Standard deviation of shock on outcome (Y)', 'placement': 'top', 'trigger': 'hover', 'title': 'sd'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_ate\">ate<\/label>\n    <input id=\"tab_design-design_arg_ate\" type=\"number\" class=\"form-control\" value=\"1\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_ate', 'popover', {'content': 'The average treatment effect', 'placement': 'top', 'trigger': 'hover', 'title': 'ate'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_h\">h<\/label>\n    <input id=\"tab_design-design_arg_h\" type=\"number\" class=\"form-control\" value=\"0\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_h', 'popover', {'content': 'Heterogeneous treatment effects by covariate (W)', 'placement': 'top', 'trigger': 'hover', 'title': 'h'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_treatment_mean\">treatment_mean<\/label>\n    <input id=\"tab_design-design_arg_treatment_mean\" type=\"number\" class=\"form-control\" value=\"1\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_treatment_mean', 'popover', {'content': 'Average outcome in treatment. Overrides ate if both specified', 'placement': 'top', 'trigger': 'hover', 'title': 'treatment_mean'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_rho_WY\">rho_WY<\/label>\n    <input id=\"tab_design-design_arg_rho_WY\" type=\"number\" class=\"form-control\" value=\"0\" min=\"-1\" max=\"1\" step=\"0.1\"/>\n  <\/div>\n  <scrip
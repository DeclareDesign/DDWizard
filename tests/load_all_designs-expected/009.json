    "tab_design-design_parameters": {
      "html": "<div>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_N\">N<\/label>\n    <input id=\"tab_design-design_arg_N\" type=\"number\" class=\"form-control\" value=\"100\" min=\"6\" step=\"1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_N', 'popover', {'content': 'Size of population of cases selected', 'placement': 'top', 'trigger': 'hover', 'title': 'N'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_prob_X\">prob_X<\/label>\n    <input id=\"tab_design-design_arg_prob_X\" type=\"number\" class=\"form-control\" value=\"0.5\" min=\"0\" max=\"1\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_prob_X', 'popover', {'content': 'Probability that X = 1 for a given case', 'placement': 'top', 'trigger': 'hover', 'title': 'prob_X'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_process_proportions\">process_proportions<\/label>\n    <input id=\"tab_design-design_arg_process_proportions\" type=\"text\" class=\"form-control\" value=\"0.25, 0.25, 0.25, 0.25\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_process_proportions', 'popover', {'content': 'Simplex denoting the proportion of cases in the population', 'placement': 'top', 'trigger': 'hover', 'title': 'process_proportions'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_prior_H\">prior_H<\/label>\n    <input id=\"tab_design-design_arg_prior_H\" type=\"number\" class=\"form-control\" value=\"0.5\" min=\"0\" max=\"1\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_prior_H', 'popover', {'content': 'Prior probability that X indeed causes Y', 'placement': 'top', 'trigger': 'hover', 'title': 'prior_H'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_p_E1_H\">p_E1_H<\/label>\n    <input id=\"tab_design-design_arg_p_E1_H\" type=\"number\" class=\"form-control\" value=\"0.8\" min=\"0\" max=\"1\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_p_E1_H', 'popover', {'content': 'Probability of observing the first piece of evidence given X indeed causes Y', 'placement': 'top', 'trigger': 'hover', 'title': 'p_E1_H'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_p_E1_not_H\">p_E1_not_H<\/label>\n    <input id=\"tab_design-design_arg_p_E1_not_H\" type=\"number\" class=\"form-control\" value=\"0.2\" min=\"0\" max=\"1\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_p_E1_not_H', 'popover', {'content': 'Probability of observing first piece of evidence given that X caused Y is not true', 'placement': 'top', 'trigger': 'hover', 'title': 'p_E1_not_H'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_p_E2_H\">p_E2_H<\/label>\n    <input id=\"tab_design-design_arg_p_E2_H\" type=\"number\" class=\"form-control\" value=\"0.3\" min=\"0\" max=\"1\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_p_E2_H', 'popover', {'content': 'Probability of observing second piece of evidence given X indeed caused Y ', 'placement': 'top', 'trigger': 'hover', 'title': 'p_E2_H'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_p_E2_not_H\">p_E2_not_H<\/label>\n    <input id=\"tab_design-design_arg_p_E2_not_H\" type=\"number\" class=\"form-control\" value=\"0\" min=\"0\" max=\"1\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_p_E2_not_H', 'popover', {'content': 'Probability of observing second piece of evidence given that X caused Y is not true.', 'placement': 'top', 'trigger': 'hover', 'title': 'p_E2_not_H'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_cor_E1E2_H\">cor_E1E2_H<\/label>\n    <input id=\"tab_design-design_arg_cor_E1E2_H\" type=\"number\" class=\"form-control\" value=\"0\" min=\"-1\" max=\"1\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_cor_E1E2_H', 'popover', {'content': 'Correlation in first and second pieces of evidence given X indeed causes Y', 'placement': 'top', 'trigger': 'hover', 'title': 'cor_E1E2_H'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_cor_E1E2_not_H\">cor_E1E2_not_H<\/label>\n    <input id=\"tab_design-design_arg_cor_E1E2_not_H\" type=\"number\" class=\"form-control\" value=\"0\" min=\"-1\" max=\"1\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_cor_E1E2_not_H', 'popover', {'content': 'Correlation in first and second pieces of evidence given that X caused Y is not true', 'placement': 'top', 'trigger': 'hover', 'title': 'cor_E1E2_not_H'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_label_E1\">label_E1<\/label>\n    <input id=\"tab_design-design_arg_label_E1\" type=\"text\" class=\"form-control\" value=\"Straw in the Wind\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_label_E1', 'popover', {'content': 'Label for the first piece of evidence', 'placement': 'top', 'trigger': 'hover', 'title': 'label_E1'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_label_E2\">label_E2<\/label>\n    <input id=\"tab_design-design_arg_label_E2\" type=\"text\" class=\"form-control\" value=\"Smoking Gun\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_label_E2', 'popover', {'content': 'Label for the second piece of evidence', 'placement': 'top', 'trigger': 'hover', 'title': 'label_E2'})}, 500)});<\/script>\n<\/div>",

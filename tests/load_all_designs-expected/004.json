    "tab_design-design_parameters": {
      "html": "<div>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_N_blocks\">N_blocks<\/label>\n    <input id=\"tab_design-design_arg_N_blocks\" type=\"number\" class=\"form-control\" value=\"1\" min=\"1\" step=\"1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_N_blocks', 'popover', {'content': 'Number of blocks', 'placement': 'top', 'trigger': 'hover', 'title': 'N_blocks'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_N_clusters_in_block\">N_clusters_in_block<\/label>\n    <input id=\"tab_design-design_arg_N_clusters_in_block\" type=\"text\" class=\"form-control\" value=\"1000\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_N_clusters_in_block', 'popover', {'content': 'Number of clusters in each block', 'placement': 'top', 'trigger': 'hover', 'title': 'N_clusters_in_block'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_N_i_in_cluster\">N_i_in_cluster<\/label>\n    <input id=\"tab_design-design_arg_N_i_in_cluster\" type=\"text\" class=\"form-control\" value=\"50\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_N_i_in_cluster', 'popover', {'content': 'Number of units per cluster sampled', 'placement': 'top', 'trigger': 'hover', 'title': 'N_i_in_cluster'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_n_clusters_in_block\">n_clusters_in_block<\/label>\n    <input id=\"tab_design-design_arg_n_clusters_in_block\" type=\"text\" class=\"form-control\" value=\"100\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_n_clusters_in_block', 'popover', {'content': 'Number of clusters to sample in each block', 'placement': 'top', 'trigger': 'hover', 'title': 'n_clusters_in_block'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_n_i_in_cluster\">n_i_in_cluster<\/label>\n    <input id=\"tab_design-design_arg_n_i_in_cluster\" type=\"text\" class=\"form-control\" value=\"10\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_n_i_in_cluster', 'popover', {'content': 'Number of units to sample in each cluster', 'placement': 'top', 'trigger': 'hover', 'title': 'n_i_in_cluster'})}, 500)});<\/script>\n  <div class=\"form-group shiny-input-container\" style=\"width: 100%;\">\n    <label for=\"tab_design-design_arg_icc\">icc<\/label>\n    <input id=\"tab_design-design_arg_icc\" type=\"number\" class=\"form-control\" value=\"0.2\" min=\"0\" max=\"1\" step=\"0.1\"/>\n  <\/div>\n  <script>$(document).ready(function() {setTimeout(function() {shinyBS.addTooltip('tab_design-design_arg_icc', 'popover', {'content': 'Intra-cluster Correlation', 'placement': 'top', 'trigger': 'hover', 'title': 'icc'})}, 500)});<\/script>\n<\/div>",
